{
  "name": "GoogleMeetExtention",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "クロコダイン",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": ["tabs"]
}
